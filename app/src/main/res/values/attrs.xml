<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="StarRatingView" tools:ignore="ResourceName">
        <attr name="starImageSize" format="dimension" />
        <attr name="starImageWidth" format="dimension" />
        <attr name="starImageHeight" format="dimension" />
        <attr name="starImagePading" format="dimension" />
        <attr name="starCount" format="integer" />
        <attr name="starNum" format="integer" />
        <attr name="starEmpty" format="reference" />
        <attr name="starFill" format="reference" />
        <attr name="starHalf" format="reference" />
        <attr name="clickable" format="boolean" />
        <attr name="harlfstart" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DraggableFlagView">
        <attr name="color1" format="color" />
    </declare-styleable>

    <declare-styleable name="SquareLayout">
        <attr name="accordTo" format="integer"/>

    </declare-styleable>

    <declare-styleable name="Loading">
        <attr name="gAutoRun" format="boolean"/>
        <attr name="gBackgroundColor" format="reference"/>
        <attr name="gBackgroundLineSize" format="integer"/>
        <attr name="gForegroundColor" format="reference"/>
        <attr name="gForegroundLineSize" format="integer"/>
        <attr name="gProgressFloat" format="float"/>


    </declare-styleable>

    <declare-styleable name="MyEnterLayout">

        <attr name="itemIcon" format="reference"/>
        <attr name="itemTitle" format="string"/>

    </declare-styleable>

</resources>